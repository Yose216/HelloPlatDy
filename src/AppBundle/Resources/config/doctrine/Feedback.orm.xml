<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Feedback" table="Feedback">
    <indexes>
      <index name="fk_Feedback_Restaurant1_idx" columns="id_restaurant"/>
      <index name="fk_Feedback_User1_idx" columns="id_user"/>
    </indexes>
    <id name="idFeedback" type="integer" column="id_feedback"/>
    <id name="idRestaurant" association-key="true"/>
    <id name="idUser" association-key="true"/>
    <field name="feedbackRating" type="integer" column="feedback_rating" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="feedbackTitle" type="string" column="feedback_title" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="feedbackComments" type="text" column="feedback_comments" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="feedbackDate" type="string" column="feedback_date" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-one field="idRestaurant" target-entity="Restaurant" fetch="LAZY">
      <join-columns>
        <join-column name="id_restaurant" referenced-column-name="id_restaurant"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idUser" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="id_user" referenced-column-name="id_user"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
