<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Restaurant" table="Restaurant">
    <id name="idRestaurant" type="integer" column="id_restaurant">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="restaurantName" type="string" column="restaurant_name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="restaurantPwd" type="string" column="restaurant_pwd" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="restaurantSalt" type="string" column="restaurant_salt" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="restaurantMail" type="string" column="restaurant_mail" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="restaurantStreet" type="string" column="restaurant_street" length="150" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="restaurantTown" type="string" column="restaurant_town" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="restaurantZipCode" type="integer" column="restaurant_zip_code" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="restaurantHoraireMonday" type="string" column="restaurant_horaire_monday" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="restaurantHoraireTuesday" type="string" column="restaurant_horaire_tuesday" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="restaurantHoraireWednesday" type="string" column="restaurant_horaire_wednesday" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="restaurantHoraireThursday" type="string" column="restaurant_horaire_thursday" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="restaurantHoraireFriday" type="string" column="restaurant_horaire_friday" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="restaurantHoraireSaturday" type="string" column="restaurant_horaire_saturday" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="restaurantHoraireSunday" type="string" column="restaurant_horaire_sunday" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="restaurantCategory" type="string" column="restaurant_category" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="restaurantDescription" type="text" column="restaurant_description" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
