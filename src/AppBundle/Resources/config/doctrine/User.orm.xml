<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\User" table="User">
    <id name="idUser" type="integer" column="id_user">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userName" type="string" column="user_name" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userPwd" type="string" column="user_pwd" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userSalt" type="string" column="user_salt" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userMail" type="string" column="user_mail" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userRole" type="string" column="user_role" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="idDiscountCode" target-entity="DiscountCode" mapped-by="idUser" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
